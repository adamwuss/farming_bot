{
  "name": "Plemiona",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://*.plemiona.pl/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "action": {
    "default_title": "Farm",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["tabs"]
}
